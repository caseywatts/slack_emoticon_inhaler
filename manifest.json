{
  "manifest_version": 2,

  "name": "Slack Emoticon Inhaler",
  "description": "Downlaods all of the emoticons on a page",
  "version": "1.1",

  "content_scripts": [
    {
      "matches": ["https://*.slack.com/customize/emoji", "https://*.slack.com/admin/emoji"],
      "js":      ["jquery.js", "inhaler.js"]
    }
  ],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "downloads"
  ]

}
